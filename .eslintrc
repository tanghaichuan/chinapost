{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "plugins": [
      "css-modules"
    ],
    "parserOptions": { 
      "ecmaVersion": 6 
    },
    "globals": {
      "__DEV__": true
    },
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "quotes":"off",
      "one-var-declaration-per-line":"off",
      "one-var":"off",
      "indent": "off",
      "prefer-const":"off",
      "comma-spacing":"off",
      "arrow-spacing":"off",
      "arrow-body-style":"off",
      "comma-dangle":"off",
      "eol-last":"off",
      "no-unused-expressions":"off",
      "react/jsx-indent":"off",
      "no-case-declarations":"off",
      "no-use-before-define":"off",
      "no-shadow":"off",
      "no-restricted-syntax":"off",
      "no-underscore-dangle":"off",
      "guard-for-in":"off",
      "no-param-reassign":"off",
      "class-methods-use-this":"off",
      "global-require":"off",
      "semi":"off",
      "arrow-parens": "off",
      "generator-star-spacing": "off",
      "import/extensions": "off",
      "import/no-extraneous-dependencies": "off",
      "linebreak-style": "off"
    }
}
